{
  "tool_name": "grep",
  "arguments": ["-r", "search_term", "/home/user/documents"],
  "working_directory": "/home/user",
  "environment_variables": {
    "PATH": "/usr/bin:/bin",
    "LANG": "en_US.UTF-8"
